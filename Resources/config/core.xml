<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="sonata.notification.iterator.mysql" class="Sonata\NotificationBundle\Iterator\MessageManagerMessageIterator">
            <argument type="service" id="sonata.notification.manager.message" />
        </service>

        <service id="sonata.notification.iterator.rabbitmq" class="Sonata\NotificationBundle\Iterator\RabbitMQMessageIterator">

        </service>

        <service id="sonata.notification.dispatcher" class="Symfony\Component\EventDispatcher\EventDispatcher" />

        <service id="sonata.notification.producer.model" class="Sonata\NotificationBundle\Producer\MessageManagerProducer" >
            <argument type="service" id="sonata.notification.manager.message" />
        </service>
    </services>
</container>